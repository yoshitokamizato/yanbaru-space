<div class="row">
    <% @users.each do |user| %>
    <div class="col-md-2 col-md-offset-1">
      <div class="card">
       <img src="<%= user.image %>" class="card-img-top">
       <div class="card-body">
         <p class="card-text"><%= user.nickname %></p>
       </div>
      </div>
    </div>
    <% end %>
  </div>
